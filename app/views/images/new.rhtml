<h1>New image</h1>


<%= error_messages_for :image %>

<% form_for(:image, :url => my_images_path, :html=>{:multipart=>true}) do |f| %>
  <%= render(:partial => 'form', :locals => { :f => f, :image => @image }) %>
  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', my_images_path %>